{
	"root": true,
	"extends": [
		"airbnb",
		"plugin:import/errors",
		"plugin:import/warnings"
	],
	"parserOptions": {
		"ecmaVersion": 7,
		"sourceType": "module"
	},
	"env": {
		"browser": true,
		"node": true,
		"mocha": true
	},
	"rules": {
    "import/no-extraneous-dependencies": "warn",
    "import/prefer-default-export": "warn"
	}
}

